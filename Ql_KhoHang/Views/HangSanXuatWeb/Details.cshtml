@model Ql_KhoHang.Dtos.HangSanXuatWebDtos
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="main-wrapper">
    <div class="page-wrapper">
        <div class="content container-fluid px-3 my-3">
            <div class="row mb-3">
                <h1>Chi tiết Hãng Sản Xuất</h1>

                <div>
                    <dl class="row">
                        <dt class="col-sm-2">Mã:</dt>
                        <dd class="col-sm-10">@Model.MaHangSanXuat</dd>

                        <dt class="col-sm-2">Tên Hãng Sản Xuất:</dt>
                        <dd class="col-sm-10">@Model.TenHangSanXuat</dd>
                    </dl>
                </div>

                <a asp-action="Index" class="btn btn-secondary">Trở về</a>
            </div>
        </div>
    </div>
</div>

